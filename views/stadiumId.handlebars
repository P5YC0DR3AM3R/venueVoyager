
  <h2>{{this.stadium}}</h2>
  <p>Team: {{this.team}}</p>
  <p>League: {{this.league}}</p>
  <p>Division: {{this.division}}</p>
  <p>City: {{this.city}}</p>
  <p>State: {{this.state}}</p>
  <div class="img-box">
  {{#if this.image}}
    <img src="{{this.image}}" alt="{{this.stadium}}">
  {{else}}
    <p>No image available</p>
  {{/if}}
  </div>
